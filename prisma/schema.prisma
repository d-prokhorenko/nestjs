generator client {
  provider     = "prisma-client"
  output = "./generated/client"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model User {
  id         String   @id @default(uuid())
  email      String   @unique
  password   String
  name       String
  createdAt  DateTime @default(now()) @map("created_at")
  updateddAt DateTime @updatedAt @map("updated_at")

  @@map("users")
}
