generator client {
  provider     = "prisma-client"
  output = "./generated/client"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model Message {
  id         String   @id @default(uuid())

  text String

  createdAt  DateTime @default(now()) @map("created_at")
  updateddAt DateTime @updatedAt @map("updated_at")

  @@map("messages")
}
